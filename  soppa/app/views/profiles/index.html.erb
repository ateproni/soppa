<h1><%= t '.title' %></h1>

<% @profiles.each do |profile| %>
  <div class="profile" >
    
    <% if profile.photo.exists? then %>
      <%= image_tag profile.photo.url(:small) %>
      <% else %>
      <p>sem foto</p>
    <% end %>
    <br />
    <%= link_to profile.user.login, profile %>
  </div>
<% end %>
